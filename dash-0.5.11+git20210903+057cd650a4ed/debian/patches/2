+LIBOBJS = 
+LIBS = 
+LTLIBOBJS = 
+MAKEINFO = ${SHELL} '/home/ubuntu/nano-grand/dash-0.5.11+git20210903+057cd650a4ed/missing' makeinfo
+MKDIR_P = /usr/bin/mkdir -p
+OBJEXT = o
+PACKAGE = dash
+PACKAGE_BUGREPORT = 
