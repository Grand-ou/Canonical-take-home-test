+PACKAGE_TARNAME = dash
+PACKAGE_URL = 
+PACKAGE_VERSION = 0.5.11
+PATH_SEPARATOR = :
+SET_MAKE = 
+SHELL = /bin/bash
+STRIP = 
+VERSION = 0.5.11
+abs_builddir = /home/ubuntu/nano-grand/dash-0.5.11+git20210903+057cd650a4ed/src
+abs_srcdir = /home/ubuntu/nano-grand/dash-0.5.11+git20210903+057cd650a4ed/src
+abs_top_builddir = /home/ubuntu/nano-grand/dash-0.5.11+git20210903+057cd650a4ed
+abs_top_srcdir = /home/ubuntu/nano-grand/dash-0.5.11+git20210903+057cd650a4ed
+ac_ct_CC = gcc
+am__include = include
+am__leading_dot = .
+am__quote = 
+am__tar = $${TAR-tar} chof - "$$tardir"
+am__untar = $${TAR-tar} xf -
+bindir = ${exec_prefix}/bin
